            require(['jquery', 'backbone'], function($, BB) {
                    $(document).ready(function () {
                        var Skin = BB.View.extend({
                            el: $('body'),
                            displayAddias: $('#eplAdDivLayer').length > 0 ? true : false,
                            isHome: false,
                            _browser_is_chrome: navigator.userAgent.toLowerCase().indexOf('chrome') > -1,
                            _browser_is_IE: navigator.appName === 'Microsoft Internet Explorer',
                            events: {
                                'click': 'clickEvent'
                            },
                            initialize: function () {
                            },
                            render: function() {
                                var left_banner = $('.skin-left');
                                var right_banner = $('.skin-right');
                                var sticky = true;
                                
                                if(left_banner && right_banner) {
                                    // Posicionar los banners laterales
                                    var window_width = window.innerWidth;
                                    if(!window_width){
                                        window_width = document.documentElement.clientWidth;
                                    }
                                    if(!window_width){
                                        window_width = document.body.clientWidth;
                                    }

                                    if(!sticky){
                                        left_banner.css({'left': this._get_left_pos(window_width, 1000) + 'px', 'position': 'absolute'});
                                        right_banner.css({'left': this._get_right_pos(window_width, 1000) + 'px', 'position': 'absolute'});
                                    }
                                    else {
                                        left_banner.css('left', this._get_left_pos(window_width, 1000) + 'px');
                                        right_banner.css('left', this._get_right_pos(window_width, 1000) + 'px');
                                    }

                                    if(this.displayAddias && this.isHome){
                                        left_banner.css('display', 'noe');
                                        right_banner.css('display', 'none');
                                        this.$el.find('.rg_wrapper').css('cursor', 'pointer');
                                        this.$el.find('#eplAdDivLayer').css('cursor', 'pointer');
                                    }
                                    else {
                                        left_banner.show();
                                        right_banner.show();
                                    }
                                }
                                return this;
                            },
                            clickEvent: function(event){
                                if(this.displayAddias && this.isHome && ($(event.target).hasClass('rg_wrapper') || $(event.target).attr('id') === 'eplAdDivLayer') || $(event.target).attr('id') === 'adDefault280953270a6b30a7'){
                                    // window.open('http://bs.serving-sys.com/BurstingPipe/adServer.bs?cn=tf&c=20&mc=click&pli=12341256&PluID=0&ord=[timestamp]');
                                }
                            },
                            _getIEversion: function(){
                                var rv = 0; // Return value assumes failure.
                                if (this._browser_is_IE){
                                    var ua = navigator.userAgent;
                                    var re  = new RegExp("MSIE ([0-9]{1,}[.0-9]{0,})");
                                    if (re.exec(ua) !== null){
                                        rv = parseFloat( RegExp.$1 );
                                    }
                                }
                                return rv;
                            },
                            /*
                            _get_left_pos: function(canvas_width, content_width){
                                var left_side = (canvas_width - content_width)/2;

                                if(this._browser_is_chrome){
                                    left_side += -23;
                                }
                                else {
                                    left_side += -23;
                                }

                                left_side = left_side - 170;

                                if(this._browser_is_IE && this._getIEversion() < 8){
                                    left_side += 21;
                                }
                                else {
                                    left_side += 15;
                                }

                                return left_side - 8;
                            },
                            _get_right_pos: function(canvas_width, content_width){
                                var right_side = (canvas_width - content_width)/2 + content_width;

                                if(this._browser_is_chrome){
                                    right_side += -23;
                                }
                                else {
                                    right_side += -23;
                                }

                                if(this._browser_is_IE && this._getIEversion() < 8){
                                    right_side += 15;
                                }
                                else {
                                    right_side += 15;
                                }

                                return right_side - 13;
                            }
                            */
                            _get_left_pos: function(canvas_width, content_width){
                                var left_side = (canvas_width - content_width)/2;

                                if(this._browser_is_chrome){
                                    left_side += -23;
                                }
                                else {
                                    left_side += -23;
                                }

                                left_side = left_side - 170;

                                if(this._browser_is_IE && this._getIEversion() < 8){
                                    left_side += 21;
                                }
                                else {
                                    left_side += 15;
                                }

                                return left_side - 8;
                            },
                            _get_right_pos: function(canvas_width, content_width){
                                var right_side = (canvas_width - content_width)/2 + content_width;

                                if(this._browser_is_chrome){
                                    right_side += -23;
                                }
                                else {
                                    right_side += -23;
                                }

                                if(this._browser_is_IE && this._getIEversion() < 8){
                                    right_side += 15;
                                }
                                else {
                                    right_side += 15;
                                }

                                return right_side - 13;
                            }
                        });
                        
                        new Skin().render();
                     });
                });

