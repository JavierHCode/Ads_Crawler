        <script type="text/javascript">
            var PREBID_TIMEOUT = 700;
            var pbjs = pbjs || {};
            pbjs.que = pbjs.que || [];

            var adUnits = [
                {
                    // DESKTOP
                    code: '950x90',
                    sizes: [[728, 90], [970, 250], [970, 90], [970, 66], [960, 90], [950, 90], [980, 90]],
                    bids: [
                        {
                            bidder: 'appnexus',
                            params: { placementId: '12802098' }
                        },
                        {
                            bidder: 'rubicon',
                            params: {
                                accountId: '14194',
                                siteId: '174064',
                                zoneId: '842142',
                                sizes: [2, 55, 57],
                                inventory: { position: 'atf' }
                            }
                        },
                        {
                            bidder: 'openx',
                            params: {
                                unit: '539857213',
                                delDomain: 'futbolsites-d.openx.net'
                            }
                        }
                    ]
                },
                {
                    //DESKTOP
                    code: '300x250_A',
                    sizes: [[300, 250], [300, 600]],
                    bids: [
                        {
                            bidder: 'appnexus',
                            params: { placementId: '12802093' }
                        },
                        {
                            bidder: 'rubicon',
                            params: {
                                accountId: '14194',
                                siteId: '174064',
                                zoneId: '842142',
                                sizes: [15, 10],
                                inventory: { position: 'atf' }
                            }
                        },
                        {
                            bidder: 'openx',
                            params: {
                                unit: '539857207',
                                delDomain: 'futbolsites-d.openx.net'
                            }
                        }
                    ]
                },
                {
                    //DESKTOP
                    code: '300x250_B',
                    sizes: [[300, 250], [300, 600]],
                    bids: [
                        {
                            bidder: 'appnexus',
                            params: { placementId: '12802094' }
                        },
                        {
                            bidder: 'rubicon',
                            params: {
                                accountId: '14194',
                                siteId: '174064',
                                zoneId: '842138',
                                sizes: [15, 10],
                                inventory: { position: 'btf' }
                            }
                        },
                        {
                            bidder: 'openx',
                            params: {
                                unit: '539857208',
                                delDomain: 'futbolsites-d.openx.net'
                            }
                        }
                    ]
                }
            ];

            pbjs.que.push(function() {
                pbjs.addAdUnits(adUnits);
                pbjs.setConfig({
                    userSync: { iframeEnabled: true }
                });
                pbjs.requestBids({
                    bidsBackHandler: sendAdserverRequest
                });
            });
        
            function sendAdserverRequest() {
                if (pbjs.adserverRequestSent) return;
                pbjs.adserverRequestSent = true;
                googletag.cmd.push(function() {
                    pbjs.que.push(function() {
                        pbjs.setTargetingForGPTAsync();
                        googletag.pubads().refresh();
                    });
                });
            }
        
            setTimeout(function() {
                sendAdserverRequest();
            }, PREBID_TIMEOUT);
        </script>

        
    <script>
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
</script>
<script async= 'async' src="https://www.googletagservices.com/tag/js/gpt.js"></script>
<script type="text/javascript" language="JavaScript">
    var desktop = (window.innerWidth >= 1000)? true : false,
    mobile  = (window.innerWidth < 1000)? true : false,
    gtagSlot    = [],//array para los slots asincrónicos
    //  DI configurations
    site    = 'redgol.cl',
    section = 'ros';

    

    
    category_id = '734';
    

    dfp                     = {};
    //dfp.debug                 = (window.location.search.indexOf("debugFsn") !== -1)? true : false; 
    dfp.debug               = false; 
    dfp.method              = 'async';
    dfp.section             = (typeof(section) == 'undefined')? 'noticias' : section; //----------------------------------------Sección por defecto
    //dfp.site              = (typeof(site) == 'undefined')? 'test' : site; //----------------------------------------Sitio por defecto
    dfp.site                = (typeof(site) == 'undefined')? 'redgol.cl' : site; //----------------------------------------Sitio por defecto
    dfp.article_id          = (typeof(article_id) == 'undefined')? '' : article_id; //En caso que falte la variable defino vacia por defecto
    dfp.category_id         = (typeof(category_id) == 'undefined')? '' : category_id; //En caso que falte la variable defino vacia por defecto
        
    dfp.ads                 = {}; 
    dfp.ads.slotString      = '/63317524/'+dfp.site+'/'+dfp.section;
    dfp.ads.defineSlots     = function() {                          //Slots por defecto
        if (typeof asiPlacements != "undefined") {
            for (var p in asiPlacements) {
                window["ASPQ_" + p] = "";
                for (var key in asiPlacements[p].data) {
                    window["ASPQ_" + p] += "PQ_" + p + "_" + key;
                }
            }
        } 
    
        if (typeof(ASPQ_DPxZet) !== "string") ASPQ_DPxZet = '';
        if (typeof(ASPQ_6D9eVr) !== "string") ASPQ_6D9eVr = '';
        if (typeof(ASPQ_OAkYMc) !== "string") ASPQ_OAkYMc = '';
    
        if (dfp.debug) {
            console.log("95050");
            console.log(typeof(ASPQ_DPxZet));
            console.log("300250");
            console.log(typeof(ASPQ_6D9eVr));
            console.log("300_Top");
            console.log(typeof(ASPQ_OAkYMc));
        }

        if (dfp.section === "home")
            dfp.ads.sections.home();
        if (dfp.section === "noticias")
            dfp.ads.sections.noticias();
        if (dfp.section === "ros")
            dfp.ads.sections.ros();
    
        dfp.ads.defineMethods();
    };
    dfp.ads.defineMethods   = function() {
        if (dfp.debug) {
            googletag.pubads().addEventListener('slotRenderEnded', function(event){
                console.log("Slot String: "+ dfp.ads.slotString);
                console.log("LineItemId: "+event.lineItemId);
                console.log("CreativeId: "+event.creativeId);
                console.log("Slot: ");
                console.log(event.isEmpty);
                console.log(event.slot);
            });
        } else {
            googletag.pubads().collapseEmptyDivs(true);
        }
        
        googletag.pubads().addEventListener('slotRenderEnded', function(event){
        if (event.slot === dfp.ads.sliderMob && event.isEmpty){
                if (dfp.ads.slideDir == 'prev'){
                    setTimeout(function(){ global.ros.slider.showPrev(); }, 400);
                }else {
                    setTimeout(function(){ global.ros.slider.showNext(); }, 400);
                }
            }
        });
        if (dfp.method == "async") {
            console.log("Async");
            googletag.pubads().enableSingleRequest();
            googletag.pubads().disableInitialLoad();
            googletag.enableServices();
        } else {
            googletag.pubads().enableSyncRendering();
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        }
    };
    dfp.ads.printSlot   = function(slotName){
        if (dfp.debug) console.log('Slot name: '+slotName);
        googletag.cmd.push(function() {
            googletag.display(slotName);
        });
    };
    
    dfp.ads.sections = {};
    dfp.ads.sections.home = function() {
        googletag.defineOutOfPageSlot(dfp.ads.slotString, 'OutOfPage2')
            .addService(googletag.companionAds())
            
            
            .setTargeting('categoryID', 734)
            
            .addService(googletag.pubads())
            .setTargeting("format", ["flotante"]);
        googletag.defineOutOfPageSlot(dfp.ads.slotString, 'OutOfPage')
            .addService(googletag.companionAds())
            
            
            .setTargeting('categoryID', 734)
            
            .addService(googletag.pubads())
            .setTargeting("format", ["skin"]);

        
        googletag.defineSlot(dfp.ads.slotString, [[160, 600], [120, 600]], '160x600_r').setTargeting('categoryID', 734)
            .addService(googletag.pubads())
            .setTargeting("pos-desktop", ["right"]);
        googletag.defineSlot(dfp.ads.slotString, [[160, 600], [120, 600]], '160x600_l').setTargeting('categoryID', 734)
            .addService(googletag.pubads())
            .setTargeting("pos-desktop", ["left"]);
        

        
            googletag.defineSlot(dfp.ads.slotString, [[728, 90]], '728x90')
            
            
            .setTargeting('categoryID', 734)
            
            .addService(googletag.pubads())
            .setTargeting("pos-desktop", ['bottom']);
        
            googletag.defineSlot(dfp.ads.slotString, [[580, 90], [468, 60]], '580x90')
            
            
            .setTargeting('categoryID', 734)
            
            .addService(googletag.pubads())
            .setTargeting("pos-desktop", ['']);
        
            googletag.defineSlot(dfp.ads.slotString, [[200, 200], [234, 60], [240, 400], [250, 250], [300, 250], [300, 600], [300, 400], [300, 450]], '300x250_A')
            
            
            .setTargeting('categoryID', 734)
            
            .addService(googletag.pubads())
            .setTargeting("pos-desktop", ['top']);
        
