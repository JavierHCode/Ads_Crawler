            var sticky = false;
            require(['jquery', 'underscore','backbone'], function ($, _, BB) {
                $(document).ready(function () { 
                    var Nav = BB.View.extend({
                        el: $('body'),
                        elementos: {
                            nav: '#base-nav',
                            header: '#base-header',
                            wrapper: '#base-wrapper',
                            rcol: '.col-300-sticky'
                        },
                        events: {
                            'click #hamburger': 'accionar',
                            'click #base-wrapper': 'comprobar'
                        },
                        initialize: function(){
                            if(sticky){
                                $('#col-300').addClass('col-300-sticky');
                            }
                            $(this.elementos.rcol).css({
                                'position': 'fixed',
                                'left': ((($(window).width() - $('#base-section').width()) / 2) + 700).toString() + 'px'
                            });
                        },
                        render: function(){
                        },
                        oculto: true,
                        funcionando: false,
                        accionar: function(){
                            if(this.funcionando) return false;
                            if(this.oculto){
                                return this.mostrar();
                            }
                            else {
                                return this.ocultar();
                            }
                        },
                        mostrar: function(){
                            this.funcionando = true;
                            this.oculto = false;
                            $(this.elementos.nav).animate({
                                left: '0'
                            }, 250);
                            $(this.elementos.header).animate({
                                left: '350px'
                            }, 250);
                            $(this.elementos.wrapper).animate({
                                left: '350px'
                            }, 250);
                            $(this.elementos.rcol).animate({
                                left: (((($(window).width() - $('#base-section').width()) / 2) + 700) + 358.5).toString() + 'px'
                            }, 250);
                            $(this.el).css({'overflow-y': 'hidden'});
                            this.funcionando = false;
                        },
                        ocultar: function(){
                            this.funcionando = true;
                            this.oculto = true;
                            $(this.elementos.nav).animate({
                                left: '-350px'
                            }, 250);
                            $(this.elementos.header).animate({
                                left: '0'
                            }, 250);
                            $(this.elementos.wrapper).animate({
                                left: '0'
                            }, 250);
                            $(this.elementos.rcol).animate({
                                left: (((($(window).width() - $('#base-section').width()) / 2) + 700) - 8.5).toString() + 'px'
                            }, 250);
                            $(this.el).css({'overflow-y': 'visible'});
                            this.funcionando = false;
                        },
                        comprobar: function(){
                            if(this.oculto) return;
                            this.ocultar();
                        }
                    });
                    new Nav().render();
                });
            });

