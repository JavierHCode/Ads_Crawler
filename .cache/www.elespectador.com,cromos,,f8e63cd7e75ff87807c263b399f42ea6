</div></div><div class="col-xs-12 col-md-12 col-lg-4"><div class="object block-dfp-roba_pagina_bot_seccion"><div id="dfp-ad-roba_pagina_bot_seccion-wrapper" class="dfp-tag-wrapper"><div class="fake_dfp" id="dfp-ad-roba_pagina_bot_seccion" class="dfp-tag-wrapper">
<script type="text/javascript">
    function slotDfp(){

      if( typeof googletag.slots["roba_pagina_bot_seccionrdt4f"] == "undefined" ){
        var mapping = googletag.sizeMapping()
                .addSize([1050, 200],[300, 250])
                .addSize([750, 200],[[320, 50], [300, 50]])
                .addSize([320, 480],[[300, 250], [320, 50], [300, 50]])
                .build();
        googletag.cmd.push(function() {
          googletag.slots["roba_pagina_bot_seccionrdt4f"] = googletag.defineSlot( "100249493/elespectador/cromos", [[320, 250], [300, 250]], "dfp-ad-roba_pagina_bot_seccionrdt4f")
                              .setTargeting('ad_group',Adomik.randomAdGroup())
                              .addService(googletag.pubads())
                                                                                                .setTargeting("Posicion", "Bot")
                                                                                                                                .setTargeting("especial", "[node:url:args:value:0]")
                                                                                                                                .setTargeting("second_section", "[term:url:args:value:1][node:url:args:value:1]")
                                                                                            .defineSizeMapping(mapping);
          googletag.display("dfp-ad-roba_pagina_bot_seccionrdt4f");
        });
      }else{
        googletag.cmd.push(function() { googletag.display("dfp-ad-roba_pagina_bot_seccionrdt4f"); });
      }
    }
    window.addEventListener("load", function() { slotDfp(); }, false); if( typeof googletag != "undefined" ){ slotDfp(); }
  </script>
</div>
